{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Information"
    }
  },
   "AllowedHosts": "*",
   "CustomSettings": {
      "DatabaseConnection": "Server=tcp:<server>;Database=<database>;User ID=<user>;Pwd=<password>;Connection Timeout=90;Max Pool Size=5000;Trusted_Connection=False;",
      "Jwt": {
         "Key": "<jwt encryption key>",
         "Issuer": "http://localhost:43219",
         "Audience": "http://localhost:43219"
      },
      "RabbitMq": {
         "Connection": "<rabbitMq connection string>",
         "Exchange": "amq.direct",
         "Query": "ToDoQuery"
      }
   }
}
